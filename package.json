{
  "name": "senars3",
  "version": "1.0.0",
  "description": "Verifiable, goal-directed, and self-reflective reasoning system with hybrid symbolic and semantic cognition",
  "main": "dist/index.js",
  "bin": {
    "senars3": "./dist/unifiedCLI.js"
  },
  "scripts": {
    "build": "npm run build:frontend && tsc",
    "start": "ts-node src/unifiedCLI.ts",
    "start:ws": "ts-node src/web/webSocketServer.ts",
    "start:web": "ts-node src/web/webServer.ts",
    "start:unified": "npm run build && node dist/web/unifiedServer.js",
    "cli": "ts-node src/unifiedCLI.ts",
    "demo": "ts-node src/testing/demo.ts",
    "test:community": "ts-node src/testing/communitySimulatorTest.ts",
    "test:community-integration": "ts-node src/testing/communitySimulatorIntegrationTest.ts",
    "test:usability": "ts-node src/testing/usabilityIntegrityTest.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest test/unit",
    "test:integration": "jest test/integration",
    "docs:usability": "cat USABILITY_ENHANCEMENTS.md",
    "docs:enhanced": "cat ENHANCED_USABILITY_FEATURES.md",
    "dev": "ts-node src/web/webServer.ts",
    "dev:unified": "ts-node src/web/unifiedServer.ts",
    "dev:frontend": "vite",
    "build:frontend": "vite build",
    "preview:frontend": "vite preview"
  },
  "keywords": [
    "ai",
    "cognitive",
    "reasoning",
    "goal-directed",
    "symbolic-ai",
    "semantic-cognition",
    "knowledge-representation",
    "cognitive-architecture",
    "artificial-intelligence",
    "nlp",
    "natural-language-processing"
  ],
  "author": "Senars3 Team",
  "license": "ISC",
  "type": "commonjs",
  "dependencies": {
    "@langchain/community": "^0.3.53",
    "@xenova/transformers": "^2.17.2",
    "chart.js": "^4.4.3",
    "chartjs-adapter-date-fns": "^3.0.0",
    "date-fns": "^3.6.0",
    "express": "^4.21.1",
    "framer-motion": "^12.23.12",
    "langchain": "^0.3.31",
    "react": "^18.3.1",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "^18.3.1",
    "react-icons": "^5.5.0",
    "ts-node": "^10.9.2",
    "uuid": "^11.1.0",
    "ws": "^8.18.3",
    "zod": "^3.25.76",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^24.3.0",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@types/uuid": "^10.0.0",
    "@types/ws": "^8.5.10",
    "@vitejs/plugin-react": "^4.3.1",
    "jest": "^29.7.0",
    "jest-util": "^30.0.5",
    "ts-jest": "^29.4.1",
    "typescript": "^5.9.2",
    "vite": "^5.4.2"
  }
}

{
  "name": "senars3",
  "version": "1.0.0",
  "description": "Verifiable, goal-directed, and self-reflective reasoning system with hybrid symbolic and semantic cognition",
  "main": "dist/index.js",
  "bin": {
    "senars3": "./dist/unifiedCLI.js"
  },
  "scripts": {
    "build": "tsc && cp src/web/newUnifiedInterface.html dist/web/ && cp src/web/unifiedInterface.html dist/web/ && cp src/web/planner.html dist/web/ && cp src/web/planner.css dist/web/ && cp -r src/web/components dist/web/",
    "build:unified": "tsc && cp src/web/newUnifiedInterface.html dist/web/ && cp src/web/unifiedInterface.html dist/web/ && cp src/web/planner.html dist/web/ && cp src/web/planner.css dist/web/ && cp -r src/web/components dist/web/",
    "start": "ts-node src/unifiedCLI.ts",
    "start:ws": "ts-node src/web/webSocketServer.ts",
    "start:web": "ts-node src/web/webServer.ts",
    "start:unified": "npm run build:unified && node dist/web/unifiedServer.js",
    "cli": "ts-node src/unifiedCLI.ts",
    "demo": "ts-node src/testing/demo.ts",
    "demo:unified": "node unifiedDemo.js",
    "test:unified": "ts-node src/testing/unifiedInterfaceTest.ts",
    "test:community": "ts-node src/testing/communitySimulatorTest.ts",
    "test:community-integration": "ts-node src/testing/communitySimulatorIntegrationTest.ts",
    "test:usability": "ts-node src/testing/usabilityIntegrityTest.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest test/unit",
    "test:integration": "jest test/integration",
    "docs:usability": "cat USABILITY_ENHANCEMENTS.md",
    "docs:enhanced": "cat ENHANCED_USABILITY_FEATURES.md",
    "dev": "ts-node src/web/webServer.ts",
    "dev:unified": "ts-node src/web/unifiedServer.ts"
  },
  "keywords": [
    "ai",
    "cognitive",
    "reasoning",
    "goal-directed",
    "symbolic-ai",
    "semantic-cognition",
    "knowledge-representation",
    "cognitive-architecture",
    "artificial-intelligence",
    "nlp",
    "natural-language-processing"
  ],
  "author": "Senars3 Team",
  "license": "ISC",
  "type": "commonjs",
  "dependencies": {
    "@langchain/community": "^0.3.53",
    "@xenova/transformers": "^2.17.2",
    "chart.js": "^4.4.3",
    "chartjs-adapter-date-fns": "^3.0.0",
    "date-fns": "^3.6.0",
    "express": "^4.21.1",
    "langchain": "^0.3.31",
    "ts-node": "^10.9.2",
    "uuid": "^11.1.0",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^24.3.0",
    "@types/uuid": "^10.0.0",
    "@types/ws": "^8.5.10",
    "jest": "^29.7.0",
    "jest-util": "^30.0.5",
    "ts-jest": "^29.4.1",
    "typescript": "^5.9.2"
  }
}
